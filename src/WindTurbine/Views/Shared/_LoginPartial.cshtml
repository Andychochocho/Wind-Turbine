@using System.Security.Claims

@if (User.IsSignedIn())
{
    @Html.ActionLink("Log Off", "LogOff", "Account")

}
else
{
    <nav class="navbar">
        <ul class="nav navbar-nav navbar-right">
            <li>
                <a href="#myModal" data-toggle="modal" data-target="#myModal">Login</a>
            </li>
        </ul>
    </nav>
    @Html.Partial("_LoginModalPartial")
}


